{% assign list = frameworks | map: 'type' | uniq %}

<h4>Test Page</h4>
{% for item in list %}
<p><strong>{{ item }}</strong>:
  {% assign framework = frameworks | where: "type", item %}
  {% for item in framework %}
  <a href="/examples/demo1/{{ item.name | slugify }}">{{ item.name }}</a> |
  {% endfor %}
</p>
{% endfor %}

